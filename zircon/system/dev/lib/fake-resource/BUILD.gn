zx_library("fake-resource") {
  sdk = "shared"
  sdk_headers = [ "lib/fake-resource/resource.h" ]
  testonly = true
  shared = true
  sources = [ "fake-resource.cc" ]
  deps = [
    "$zx/system/dev/lib/fake-object",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zircon-internal",
    "$zx/system/ulib/zx",
  ]
  configs += [ "$zx_build/public/gn/config:static-libc++" ]
}
